<table class="table table-striped table-bordered bootstrap-datatable datatable responsive">
  <thead>
    <tr>
      <th>#</th>
      <th>Name</th>
      <th>Address</th>
      <th>Phone</th>
    </tr>
  </thead>
  <tbody>
    <% item.suppliers.each_with_index do |supplier, index| %>
      <tr>
        <td class="center"><%= index+1 %></td>
        <td class="center"><%= link_to supplier.first_name, supplier_path(supplier) %></td>
        <td class="center"><%= supplier.address %></td>
        <td class="center"><%= supplier.phone_number %></td>
      </tr>
    <% end %>
  </tbody>
</table>
